{
  "name": "@automation/browser-automation",
  "version": "0.0.9",
  "main": "./dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://github.com/markkizz/automate-my-boring-stuff-js.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "dev:bot": "nodemon --watch \"**/*\" -e ts,tsx --exec ts-node --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "build": "cross-env NODE_ENV=production ttsc -p .",
    "clean": "rm -rf ./dist",
    "publish:registry": "yarn publish"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "lodash.random": "^3.2.0",
    "puppeteer": "^10.2.0"
  },
  "devDependencies": {
    "@types/lodash.random": "^3.2.6",
    "@types/node": "^16.9.1",
    "@types/puppeteer": "^5.4.4",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.4.3",
    "typescript-transform-paths": "^3.3.1"
  }
}
